{% extends 'details/detailslayout.html.twig' %}
{% block title %} {{ indoor.name }}{% endblock %}

{% block photo %} {{asset(indoor.photo)}} {% endblock %}
{% block card %}


    <h5 class="card-title">{{indoor.name }} </h5>
    <p class="card-text">Description : {{indoor.description }} </p>
    <p class="card-text">Age min : {{indoor.agemin }} </p>
    <p class="card-text">Age max : {{indoor.agemax }}</p>
    <p class="card-text">Eco friendly : {{indoor.ecofriendly }}</p>
    <p class="card-text">Target :  </p>
    <p class="card-text">Price min: {{indoor.pricemin }}</p>
    <p class="card-text">Price max: {{indoor.pricemin }}</p>
    <p class="card-text"><small class="text-muted">Added By: {{indoor.user }} </small></p>
{% endblock %}